{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "Function": "Debug",
            "Host.Results": "Debug",
            "Host.Aggregator": "Debug",
            "Host.Triggers": "Debug",
            "Host.General": "Debug"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    }
}